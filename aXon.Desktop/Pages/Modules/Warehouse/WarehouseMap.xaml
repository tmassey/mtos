<desktop:AxonScreen                      

    x:Class="aXon.Desktop.Pages.Modules.Warehouse.WarehouseMap"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:desktop="clr-namespace:aXon.Desktop"
    mc:Ignorable="d" 
             >
    <Viewbox Stretch="Fill" HorizontalAlignment="Left"  VerticalAlignment="Top" Style="{StaticResource ContentRoot}">
        <Canvas x:Name="Canvas" Background="Transparent" Margin="0" Width="810"
                        Height="810" HorizontalAlignment="Left"
                        VerticalAlignment="Top">

            <Canvas.LayoutTransform>
                <ScaleTransform ScaleY="1" />
            </Canvas.LayoutTransform>


        </Canvas>
    </Viewbox>
</desktop:AxonScreen>
